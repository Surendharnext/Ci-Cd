currentBuild.displayName = "dev-#"+currentBuild.displayName
pipeline{
    agent any 
    environment{
        PATH="/PATH=/opt/apache-maven-3.9.0/bin:$PATH"
    }
    stages{
        stage('Git Checkout'){
            steps{
               git credentialsId: 'maven-build', url: 'https://github.com/Surendharnext/Ci-Cd'
            }
        }
        stage('mvn build'){
            steps{
                sh "mvn clean package"
            }
        }   
    }
}
